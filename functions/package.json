{
  "name": "omniy-functions",
  "version": "1.0.0",
  "description": "Omniy Instagram予約投稿アプリ - Cloud Functions",
  "main": "lib/index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:rules": "firebase emulators:exec --only firestore 'npm run test:firestore-rules'",
    "test:firestore-rules": "jest --testMatch='**/*.rules.test.js'"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.9.0",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0",
    "nodemailer": "^6.9.0",
    "socks-proxy-agent": "^8.0.5",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.0",
    "firebase-functions-test": "^3.1.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "firebase",
    "functions",
    "instagram",
    "scheduling",
    "typescript"
  ],
  "author": "Omniy Team",
  "license": "MIT",
  "private": true
}
