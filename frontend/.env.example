# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Facebook App Configuration
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_FACEBOOK_APP_VERSION=v18.0

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_key

# Firebase Emulator
VITE_ENABLE_FIREBASE_EMULATOR=true
VITE_FIRESTORE_EMULATE=true
VITE_STORAGE_EMULATE=true
VITE_AUTH_EMULATE=true

# Multi-SNS Platform Control
VITE_ENABLE_REAL_POST=false
VITE_ENABLE_MULTI_PLATFORM=true
VITE_ENABLE_X=false
VITE_ENABLE_FACEBOOK=false
VITE_ENABLE_TIKTOK=false
VITE_ENABLE_LINKEDIN=false

# Environment
VITE_APP_ENV=development