# Staging Environment Variables
VITE_APP_ENV=staging
VITE_APP_NAME=Omniy (Staging)
VITE_APP_VERSION=1.0.0-staging

# Firebase Configuration (Staging)
VITE_FIREBASE_API_KEY=your-staging-api-key
VITE_FIREBASE_AUTH_DOMAIN=omniy-staging.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=omniy-instagram-scheduler-staging
VITE_FIREBASE_STORAGE_BUCKET=omniy-instagram-scheduler-staging.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# API Endpoints
VITE_API_BASE_URL=https://us-central1-omniy-instagram-scheduler-staging.cloudfunctions.net
VITE_INSTAGRAM_GRAPH_API_URL=https://graph.instagram.com

# Stripe Configuration (Test)
VITE_STRIPE_PUBLIC_KEY=pk_test_your-stripe-test-key

# Analytics & Monitoring
VITE_ANALYTICS_ENABLED=false
VITE_SENTRY_DSN=your-staging-sentry-dsn

# Feature Flags
VITE_FEATURE_PERFORMANCE_MONITORING=true
VITE_FEATURE_PWA=true
VITE_FEATURE_I18N=true