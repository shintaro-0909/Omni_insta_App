# Production Environment Variables
VITE_APP_ENV=production
VITE_APP_NAME=Omniy
VITE_APP_VERSION=1.0.0

# Firebase Configuration (Production)
VITE_FIREBASE_API_KEY=your-production-api-key
VITE_FIREBASE_AUTH_DOMAIN=omniy-prod.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=omniy-instagram-scheduler
VITE_FIREBASE_STORAGE_BUCKET=omniy-instagram-scheduler.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# API Endpoints
VITE_API_BASE_URL=https://us-central1-omniy-instagram-scheduler.cloudfunctions.net
VITE_INSTAGRAM_GRAPH_API_URL=https://graph.instagram.com

# Stripe Configuration (Production)
VITE_STRIPE_PUBLIC_KEY=pk_live_your-stripe-public-key

# Analytics & Monitoring
VITE_ANALYTICS_ENABLED=true
VITE_SENTRY_DSN=your-sentry-dsn
VITE_GTM_ID=GTM-XXXXXXX

# Feature Flags
VITE_FEATURE_PERFORMANCE_MONITORING=true
VITE_FEATURE_PWA=true
VITE_FEATURE_I18N=true